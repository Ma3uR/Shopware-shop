<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="dv_compensation_report_backend.subscriber.templates" class="DvCompensationReportBackend\Subscriber\TemplateRegistration">
            <argument>%dv_compensation_report_backend.plugin_dir%</argument>
            <argument type="service" id="template"/>
            <tag name="shopware.event_subscriber"/>
        </service>

        <service class="DvCompensationReportBackend\Services\CompensationReportService" id="dv_compensation_report_backend.services.compensation_report_service" public="true"/>

    </services>
</container>
